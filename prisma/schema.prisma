generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int    @id @default(autoincrement())
  username String @unique @db.VarChar(50)
  email    String @unique @db.VarChar(20)
  password String
}

model view2D {
  id        Int      @id @default(autoincrement())
  fileName  String
  fileURL   String
  version   Int
  createdAt DateTime @default(now())
}

model view3D {
  id        Int      @id @default(autoincrement())
  fileName  String
  fileURL   String
  version   Int
  createdAt DateTime @default(now())
}

model viewBOQ {
  id        Int      @id @default(autoincrement())
  fileName  String
  fileURL   String
  version   Int
  createdAt DateTime @default(now())
}
